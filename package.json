{
  "name": "biolab-logistik-planner",
  "version": "13.14.0",
  "description": "Biolab Logistik Planner - Schedule, Messaging and Waste Management",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "start:minimal": "node server-minimal.js",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "echo 'Build handled by nixpacks.toml'",
    "build:client": "cd client && CI=false npm run build",
    "heroku-postbuild": "npm run build:client",
    "migrate": "node server/database/migrations.js migrate",
    "migrate:rollback": "node server/database/migrations.js rollback",
    "migrate:status": "node server/database/migrations.js status",
    "migrate:create": "node server/database/migrations.js create",
    "migrate:pg": "node server/runMigrations.js",
    "migrate:062": "node server/scripts/apply-migration-062.js",
    "seed": "node server/database/seeds.js",
    "seed:clear": "node server/database/seeds.js --clear",
    "db:reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "db:rebuild": "node server/scripts/resetDatabase.js"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "easymde": "^2.20.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "multer-storage-cloudinary": "^4.0.0",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "openai": "^4.77.3",
    "pg": "^8.16.3",
    "pg-pool": "^3.10.1",
    "qrcode": "^1.5.4",
    "react-beautiful-dnd": "^13.1.1",
    "react-markdown": "^10.1.0",
    "react-simplemde-editor": "^5.2.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "colors": "^1.4.0",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1",
    "socket.io-client": "^4.8.1"
  },
  "engines": {
    "node": ">=20.x"
  }
}
